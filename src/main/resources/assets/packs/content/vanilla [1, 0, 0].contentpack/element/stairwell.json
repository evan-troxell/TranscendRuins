{
    "metadata": {
        "identifier": "TranscendRuins:stairwell"
    },
    "attributes": {
        "modelX": "TranscendRuins:stairwell",
        "animationControllerX": "TranscendRuins:stairwell",
        "renderMaterialX": "TranscendRuins:opaque",
        "textureX": "element.stairwell",

        "model": "TranscendRuins:box",
        "animationController": "TranscendRuins:box",
        "renderMaterial": "TranscendRuins:opaque",
        "texture": "element.crate",

        "inventory": {
            "ui": {
                "slotSize": 80,
                "grid": {
                    "x": 0,
                    "y": 0,
                    "rows": 1,
                    "cols": 1
                }
            },
            "grid": {
                "slots": 1
            }
        }
    },

    "permutations": {
        "passagewayInteract": {
            "interaction": [
                {
                    "type": "inventory"
                },
                {
                    "type": "passageway",
                    "position": [1, 1, 1]
                }
            ]
        },

        "destroy": {
        }
    },
    "events": {
        "destroy": {
            "permutations": {
                "add": [ "destroy" ],
                "remove": [ "passagewayInteract" ]
            }
        },

        "TranscendRuins:onInteract": {
            "run": [ "destroy" ]
        },

        "TranscendRuins:onInitialized": {
            "permutations": {
                "add": [ "passagewayInteract" ]
            }
        }
    }
}