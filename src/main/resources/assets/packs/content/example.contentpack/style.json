{
    ".exampleButton": {
        "width": "fitContent",
        "color": "#303030",
        "backgroundColor": "#C0C0C8",
        "border": {
            "style": "solid",
            "width": "2px",
            "color": "#101010"
        },
        "borderRadius": "8px",
        "fontSize": "18px",
        "padding": "10px"
    },
    ".exampleButton text": {
        "width": "fitContent"
    },
    ".exampleButton:hover": {
        "color": "#202020",
        "backgroundColor": "#E0E0E8",
        "border": {
            "style": "solid",
            "width": "2px",
            "color": "#202020"
        }
    },
    ".exampleButton:active": {
        "color": "#101010",
        "backgroundColor": "#A0A0A8",
        "border": {
            "style": "solid",
            "width": "2px",
            "color": "#303030"
        }
    },
    ".trIcon": {
        "x": "28px",
        "y": "15px",
        "width": "100px",
        "height": "100px",
        "iconFit": "contain",
        "border": {
            "style": "solid",
            "width": "2px",
            "color": "#101010"
        }
    },
    ".trademark": {
        "y": "100% - 15px",
        "height": "fitContent",
        "origin": [
            "0%",
            "100%"
        ],
        "fontSize": "16px",
        "color": "#202030",
        "textAlign": "center"
    }
}